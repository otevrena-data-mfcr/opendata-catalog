{
  "name": "@otevrena-data-mfcr/opendata-catalog",
  "version": "1.0.0",
  "scripts": {
    "build": "ng build --prod --outputHashing=none",
    "dev": "ng serve",
    "package": "rm -f package/*.js package/*.css && cat dist/*.js > package/catalog.js && cp dist/3rdpartylicenses.txt package/",
    "update-portal": "npm run build && npm run package && rm -fr ../opendata-portal/catalog/package && cp ./package ../opendata-portal/catalog/ -R",
    "prepublishOnly": "npm run build && npm run package"
  },
  "main": "./package/catalog.js",
  "dependencies": {
    "@angular/animations": "~10.0.11",
    "@angular/common": "~10.0.11",
    "@angular/compiler": "~10.0.11",
    "@angular/core": "~10.0.11",
    "@angular/elements": "^10.0.11",
    "@angular/forms": "~10.0.11",
    "@angular/platform-browser": "~10.0.11",
    "@angular/platform-browser-dynamic": "~10.0.11",
    "@angular/router": "~10.0.11",
    "@types/yamljs": "^0.2.31",
    "@webcomponents/custom-elements": "^1.4.3",
    "bootstrap": "^4.5.3",
    "document-register-element": "^1.7.2",
    "highlight.js": "^10.1.2",
    "ngx-highlightjs": "^4.1.1",
    "otevrene-formalni-normy-dts": "^0.1.12",
    "pretty-bytes": "^5.3.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "yamljs": "^0.3.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.7",
    "@angular/cli": "~10.0.7",
